doctype html
html
	head
		meta(charset="utf-8")
		link(rel="stylesheet", type="text/css", href="css/main.min.css")
		link(rel="shortcut icon", type="image/x-icon", href="favicon.ico")
		title #{title}
		base(target="_blank")
	body
		// header
		include header
		// main
		div.main-area
			block content
		// footer
		include footer
		if (js)
			- for (var i = 0; i < js.length; i++)
				script(type="text/javascript", src="js/#{js[i]}")